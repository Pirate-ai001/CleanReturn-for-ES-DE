; =========================================
; CleanReturn - Global Configuration
; Edit this file once to match your system.
; Copy to: CleanReturn.config
; =========================================

[SCRIPTS]
; Path where CleanReturn.ps1 lives
; Example: D:\ES-DE\Scripts\CleanReturn
SCRIPTS_PATH=PATH_TO_YOUR_ESDE_SCRIPTS\CleanReturn

[STEAM]
; Path to Steam executable
; Example: C:\Program Files (x86)\Steam\steam.exe
STEAM_EXE_PATH=PATH_TO_YOUR_STEAM\steam.exe

[EPIC]
; Path where Epic Games shortcuts (.url files) are stored
; Example: D:\ES-DE\Roms\Windows\Epic Shortcuts
EPIC_SHORTCUTS_PATH=PATH_TO_YOUR_EPIC_SHORTCUTS
